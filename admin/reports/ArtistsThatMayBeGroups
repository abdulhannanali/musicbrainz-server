use FindBin;
use lib "$FindBin::Bin/../../cgi-bin";

use strict;
use warnings;

package ArtistsThatMayBeGroups;
use base qw( MusicBrainz::Server::ReportScript );

sub GatherData
{
	my $self = shift;

	my $sql = $self->SqlObj;

	$self->GatherDataFromQuery(<<'EOF');
		SELECT DISTINCT ON(artist.sortname, artist.id) artist.id, artist.name, artist.type 
		  FROM artist, l_artist_artist 
		 WHERE (artist.type != 2 OR artist.type IS NULL) 
		   AND artist.id=l_artist_artist.link1 
		   AND (link_type=2 OR link_type=11)
      ORDER BY artist.sortname, artist.id
EOF
}

__PACKAGE__->new->RunReport;

# eof ArtistsThatMayBeGroups.pm

<%perl>

   my ($i, $p);

   $p = '<a href="/browseartists.html?index=1">#</a> ';
   for($i = 0; $i < 26; $i++)
   {
       $p .= '<a href="/browseartists.html?index=' . chr($i + 65) . '">' .
              chr($i + 65) . "</a> ";
       if (($i + 1) % 13 == 0)
       {
           $p .= "<br>\n";
       }
   }

   mc_out($p);

</%perl>

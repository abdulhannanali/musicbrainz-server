[% IF entity.type == "url" %]
  <a href="[% entity.url | url %]">[% entity.url | html %]</a>
  [ <a href="[% c.uri_for('/uri', entity.mbid) %]">info</a> ]
[% ELSE %]
  <a href="[% c.uri_for(entity.type, entity.mbid) %]">
    <span class="[% entity.type %]_icon">
    [% entity.name | html %]
    </span>
  </a>
[% END %]
